{
	"name": "bauhaus",
	"version": "0.0.0",
	"license": "MIT",
	"private": true,
	"workspaces": [
		"packages/*",
		"app"
	],
	"scripts": {
		"start": "yarn workspace Bauhaus run start",
		"build": "yarn workspaces run build",
		"build-insee": "yarn workspaces run build-insee",
		"test:coverage": "yarn workspaces run test:coverage --env=jest-environment-jsdom-sixteen",
		"pre-push": "yarn workspaces run lint && yarn test:coverage",
		"generate-entrypoint": "node scripts/generate-entrypoint.js",
		"prepare": "husky install"
	},
	"resolutions": {
		"@inseefr/wilco": "0.0.7",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "5.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.17.4",
		"@babel/preset-env": "^7.16.11",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^14.3.0",
		"@types/jest": "26.0.16",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^27.5.1",
		"babel-loader": "8.1.0",
		"babel-polyfill": "^6.26.0",
		"eslint": "^8.10.0",
		"eslint-config-prettier": "^8.4.0",
		"eslint-config-react-app": "^7.0.0",
		"eslint-plugin-flowtype": "^8.0.3",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jest-dom": "^4.0.1",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.29.2",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-testing-library": "^5.0.5",
		"husky": "^7.0.4",
		"jest": "26.6.0",
		"jest-localstorage-mock": "^2.4.19",
		"lcov-result-merger": "3.1.0",
		"node-fetch": "^3.2.5",
		"prettier": "^2.5.1",
		"webpack": "^5.70.0",
		"whatwg-fetch": "^3.6.2"
	},
	"dependencies": {
		"@babel/preset-react": "^7.16.7",
		"dompurify": "2.2.9",
		"jest-environment-jsdom-sixteen": "^1.0.3",
		"prop-types": "^15.8.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-redux": "^7.2.6",
		"react-router-dom": "5.2.0",
		"redux": "4.1.2",
		"rollup": "^2.70.1"
	},
	"prettier": {
		"bracketSpacing": true,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5",
		"useTabs": true
	}
}
