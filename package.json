		"react-sortable-tree": "2.8.0",
		"redux": "^4.2.0",
		"redux-thunk": "^2.4.2",
		"zod": "^3.22.4"
	},
	"prettier": {
		"bracketSpacing": true,
		"semi": true,
		"singleQuote": true,
		"useTabs": true
	}
}
