{
	"name": "bauhaus",
	"version": "3.5.0",
	"license": "MIT",
	"private": true,
	"workspaces": [
		"packages/*",
		"app"
	],
	"scripts": {
		"start": "cross-env NODE_OPTIONS=--openssl-legacy-provider  yarn workspace Bauhaus run start",
		"build": "cross-env NODE_OPTIONS=--openssl-legacy-provider  yarn workspaces run build",
		"build-insee": "cross-env NODE_OPTIONS=--openssl-legacy-provider  yarn workspaces run build-insee",
		"test:coverage": "yarn workspaces run test:coverage",
		"pre-push": "yarn workspaces run lint && yarn test:coverage",
		"generate-entrypoint": "node scripts/generate-entrypoint.js",
		"prepare": "husky install"
	},
	"resolutions": {
		"@inseefr/wilco": "0.0.7",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "5.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.17.4",
		"@babel/preset-env": "^7.16.11",
		"@testing-library/jest-dom": "^6.2.0",
		"@testing-library/react": "^14.1.2",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^28.1.6",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^27.5.1",
		"babel-loader": "8.1.0",
		"babel-polyfill": "^6.26.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-react-app": "^7.0.1",
		"eslint-plugin-flowtype": "^8.0.3",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jest": "^27.6.3",
		"eslint-plugin-jest-dom": "^5.1.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-testing-library": "^6.2.0",
		"husky": "^9.0.10",
		"jest": "26.6.0",
		"prettier": "^2.8.0",
		"webpack": "^5.75.0",
		"whatwg-fetch": "^3.6.2"
	},
	"dependencies": {
		"@babel/preset-react": "^7.16.7",
		"@sentry/react": "^7.59.3",
		"@tanstack/react-query": "^4.12.0",
		"dompurify": "2.2.9",
		"prop-types": "^15.8.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.34.2",
		"react-redux": "^7.2.6",
		"react-router-dom": "5.2.0",
		"redux": "^4.2.0",
		"rollup": "^2.70.1",
		"zod": "^3.22.4"
	},
	"prettier": {
		"bracketSpacing": true,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5",
		"useTabs": true
	}
}
