{
  "name": "bauhaus",
  "version": "4.19.0-rc0",
  "type": "module",
  "description": "Web application for the management of concepts, classifications and other statistical objects",
  "repository": {
    "type": "git",
    "url": "https://github.com/InseeFr/Bauhaus"
  },
  "license": "MIT",
  "author": "INSEE (http://www.insee.fr)",
  "private": true,
  "scripts": {
    "pre-script": "cross-env NODE_OPTIONS=--openssl-legacy-provider",
    "lint": "oxlint -c oxlint.json && npx stylelint \"src/**/*.css\"",
    "test": "vitest",
    "test:coverage": "cross-env CI=true vitest run --coverage",
    "start": "pnpm pre-script vite",
    "build": "pnpm pre-script vite build && pnpm create-zip",
    "create-zip": "cd build && bestzip build.zip *",
    "pre-push": "pnpm format:check && pnpm lint && pnpm test:coverage -- --allowOnly false",
    "prepare": "husky",
    "_format": "oxfmt src",
    "format": "pnpm _format",
    "format:check": "pnpm _format --check",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-dom": "18.3.1",
      "@types/react": "18.2.79",
      "@types/react-dom": "18.2.25",
      "loader-utils": "2.0.4",
      "shell-quote": "1.8.1"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "core-js",
      "esbuild",
      "sharp",
      "@oxlint/darwin-universal"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^22.9.0",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser": "4.0.15",
    "@vitest/coverage-istanbul": "^4.0.15",
    "bestzip": "^2.2.1",
    "cross-env": "^7.0.3",
    "esbuild-plugin-react-virtualized": "^1.0.4",
    "happy-dom": "^15.11.7",
    "husky": "^9.1.6",
    "oxfmt": "^0.18.0",
    "oxlint": "^1.24.0",
    "playwright": "^1.55.1",
    "prop-types": "^15.8.1",
    "react-app-polyfill": "^3.0.0",
    "sass": "^1.80.6",
    "stylelint": "^16.11.0",
    "stylelint-config-standard": "^36.0.1",
    "vite": "^7.3.0",
    "vite-envs": "^4.6.2",
    "vite-plugin-csp-guard": "^3.0.0",
    "vite-tsconfig-paths": "^6.0.1",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.59.20",
    "@textlint/markdown-to-ast": "^15.2.2",
    "@uiw/react-md-editor": "^4.0.4",
    "dayjs": "^1.11.6",
    "dompurify": "3.3.1",
    "draft-js": "0.11.7",
    "draft-js-export-html": "1.4.1",
    "draft-js-import-html": "1.2.1",
    "draftjs-md-converter": "1.5.2",
    "events": "^3.3.0",
    "file-saver": "1.3.3",
    "highlight.js": "^11.11.1",
    "i18next": "^25.5.3",
    "oidc-spa": "^5.3.0",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.7",
    "query-string": "^9.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-draft-wysiwyg": "1.15.0",
    "react-dropzone": "11.0.1",
    "react-i18next": "^16.0.0",
    "react-modal": "^3.14.4",
    "react-router-dom": "6.27.0",
    "react-sliding-side-panel": "1.1.8",
    "typescript": "5.7.3",
    "zod": "^4.0.0"
  }
}
